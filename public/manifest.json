{
  "manifest_version": 2,

  "name": "Bank credentials",
  "description": "Saves default logins and fills in fields on ebank page.",
  "version": "1.0.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Credentials"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "https://e.seb.lt/*",
    "https://swedbank.lt/*", "https://www.swedbank.lt/*",
    "https://ib.dnb.lt/*",
    "activeTab", "declarativeContent", "storage", "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
  {
    "matches": [
      "https://e.seb.lt/*",
      "https://swedbank.lt/*", "https://www.swedbank.lt/*",
      "https://ib.dnb.lt/*"
    ],
    "js": ["content.js"]
  }]
}